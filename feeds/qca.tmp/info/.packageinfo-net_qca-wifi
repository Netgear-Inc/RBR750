Config:
	menu "custom source directory"
		depends on PACKAGE_kmod-qca-wifi-akronite-perf
	config qca-wifi_USE_CUSTOM_SOURCE_DIR
		depends on PACKAGE_kmod-qca-wifi-akronite-perf
		bool "Use custom source directory"
		default n
	config qca-wifi_CUSTOM_SOURCE_DIR
		depends on qca-wifi_USE_CUSTOM_SOURCE_DIR
		string "Custom source directory"
		default ""
	endmenu
	menu "overwrite package version"
		depends on PACKAGE_kmod-qca-wifi-akronite-perf
	config qca-wifi_USE_CUSTOM_VERSION
		depends on PACKAGE_kmod-qca-wifi-akronite-perf
		bool "Use custom package version"
		default n

	config qca-wifi_CUSTOM_VERSION
		depends on qca-wifi_USE_CUSTOM_VERSION
		string "qca-wifi version as string (default version: g24328de-dirty)"
		default "g24328de-dirty"

	endmenu
if TARGET_ipq_ipq40xx
   config WIFI_TARGET_WIFI_2_0
	bool "Enable WIFI_2_0 support for IPQ40xx platform"
	default y
	help
	   This option enables support for WiFi 2.0 in boards
   config WIFI_TARGET_WIFI_3_0
	bool "Enable WIFI_3_0 support for IPQ806x platform"
	default y
	help
	   This option enables support for WiFi 3.0 in boards
endif
if TARGET_ar71xx
   config WIFI_TARGET_WIFI_2_0
	bool "Enable WIFI_2_0 support for ar71xx platform"
	default y
	help
	   This option enables support for WiFi 2.0 in boards
   config WIFI_TARGET_WIFI_3_0
	bool "Enable WIFI_3_0 support for ar71xx platform"
	default y
	help
	   This option enables support for WiFi 3.0 in boards
endif
